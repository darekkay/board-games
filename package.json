{
  "name": "board-games",
  "version": "1.0.0",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "handlebars": "4.1.0",
    "rc-slider": "8.6.6",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-scripts": "2.1.5",
    "react-tooltip": "3.9.2"
  },
  "devDependencies": {
    "gulp": "4.0.0",
    "gulp-yaml": "2.0.3",
    "why-did-you-update": "1.0.6"
  },
  "homepage": "https://darekkay.com/board-games",
  "private": true,
  "scripts": {
    "build": "npm run yaml-convert && react-scripts build && npm run list-all",
    "eject": "react-scripts eject",
    "format": "prettier --write -- \"{src,public}/**/*.{js,jsx,json,css}\"",
    "list-all": "node scripts/list-all.js",
    "start": "npm run yaml-convert && react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "yaml-convert": "gulp convertYaml"
  }
